{
  "name": "buildless-site",
  "version": "1.0.0",
  "description": "A collection of sites and apps built based on ES modules",
  "repository": "https://github.com/hwclass/buildless-site",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "es-dev-server --port 3000 --watch --node-resolve --open --app-index index.html",
    "postinstall": "rm -rf web_modules && node_modules/.bin/pika-web install",
    "now-build": "node_modules/.bin/pika-web install"
  },
  "keywords": [],
  "author": "hwclass",
  "license": "ISC",
  "dependencies": {
    "@vaadin/router": "^1.6.0",
    "htm": "^2.2.1",
    "husky": "^3.1.0",
    "lit-element": "^2.2.1",
    "preact": "^10.1.1",
    "pretty-quick": "^2.0.1"
  },
  "devDependencies": {
    "@pika/cli": "^0.2.0",
    "@pika/web": "latest",
    "es-dev-server": "^1.32.0",
    "prettier": "^1.19.1"
  },
  "@pika/web": {
    "webDependencies": [
      "htm",
      "preact"
    ]
  },
  "browserslist": ">0.75%, not ie 11, not op_mini all",
  "optionalDependencies": {
    "fsevents": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
