{
  "redirects": [{ "source": "^/favicon.ico$", "destination": "/favicon.ico" }],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ],
  "github": {
    "enabled": true
  }
}
